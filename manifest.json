{
  "manifest_version": 3,
  "name": "Toolware Chrome",
  "version": "0.0.1",

  "action": {
    "popup": "popup.htm"
  },
  "default_locale": "en",
  "description": "A plain text description",
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "64": "64.png",
    "128": "128.png"
  },

  "author": "TheGreatFoxxy",
  "background": {
    "service_worker": "service.js"
  },
  "homepage_url": "https://github.com/TheGreatFoxxy/Toolware-Chrome-Extension",
  "incognito": "spanning",
  "offline_enabled": true,
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+Alt+W",
        "mac": "Command+Alt+W",
        "chromeos": "Ctrl+Alt+W",
        "linux": "Ctrl+Alt+W"
      }
  "permissions": ["tabs"],
  "update_url": "http://path/to/updateInfo.xml",
  "version_name": "pre-alpha"
}
